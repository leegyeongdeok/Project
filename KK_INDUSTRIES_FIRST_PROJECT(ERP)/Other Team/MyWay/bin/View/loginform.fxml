<AnchorPane prefHeight="600.0" style="-fx-background-color: fffacd;" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox layoutY="241.0" prefHeight="359.0" prefWidth="420.0" spacing="10.0" style="-fx-background-color: fffacd;">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <children>
                <AnchorPane prefHeight="300.0" prefWidth="326.0" style="-fx-background-color: fffacd;">
                    <children>
                        <VBox alignment="CENTER" prefHeight="300.0" prefWidth="100.0" spacing="20.0">
                            <children>
                                <Label text="이름" underline="true"/>
                                <Label text="생년월일" underline="true"/>
                                <Label text="아이디" underline="true"/>
                                <Label text="비밀번호" underline="true"/>
                                <Label text="비밀번호확인" underline="true"/>
                                <Label text="전화번호" underline="true"/>
                            </children>
                        </VBox>
                        <VBox alignment="CENTER_LEFT" layoutX="100.0" layoutY="38.0" maxWidth="300.0" minWidth="191.0"
