                                <PasswordField id="memberPassword2" maxWidth="279.0" prefHeight="29.0" prefWidth="279.0"
                                               promptText="영문 숫자 기호(최대 10자)"/>
                                <TextField id="memberPhone" maxWidth="344.0" prefHeight="29.0" prefWidth="344.0"
                                           promptText="숫자 입력(최대 13자)"/>
                            </children>
                        </VBox>
                        <Button id="btnCheck" layoutX="332.0" layoutY="114.0" mnemonicParsing="false" text="중복확인"/>
                    </children>
                </AnchorPane>
                <HBox alignment="CENTER" spacing="10.0">
                    <children>
                        <Button id="btnMember" mnemonicParsing="false" prefWidth="80.0" text="가입"/>
                        <Button id="btnMemberCencel" mnemonicParsing="false" prefWidth="80.0" text="취소"/>
                    </children>
                </HBox>
            </children>
        </VBox>
        <ImageView fitHeight="241.0" fitWidth="432.0" pickOnBounds="true">
