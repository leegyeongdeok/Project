                            <children>
                                <TextField id="memberName" promptText="한글 입력(최대 5자)"/>
                                <DatePicker id="memberDate" prefHeight="29.0" prefWidth="235.0"/>
                                <TextField id="memberID" maxWidth="298.0" prefHeight="29.0" prefWidth="298.0"
